<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Template.AdminLTE</id>
        <version>1.0.0</version>
        <authors>Almsaeed Studio</authors>
        <owners>Valdis ZobÄ“la</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>
            A fully responsive admin template. Based on Bootstrap 3 framework.
            Highly customizable and easy to use.
            Fits many screen resolutions from small mobile devices to large desktops.
        </description>
        <dependencies>
            <dependency id="jQuery" version="2.0.3" />
            <dependency id="jquery.TypeScript.DefinitelyTyped" version="0.4.7" />
            <dependency id="Twitter.Bootstrap.Less" version="3.0.3" />
        </dependencies>
    </metadata>
    <files>
        <!-- Template styles -->
        <file src="less\*"
              exclude="*\login_and_register.less;*\lockscreen.less;*\invoice.less;*\iCheck.less;*\404_500_errors.less"
              target="content\Content\AdminLTE" />

        <!-- Template fonts -->
        <file src="fonts\*" target="content\Content\fonts" />
    
        <!-- Plugins -->
        <file src="js\plugins\timepicker\*.js" target="content\Scripts\vendor\timepicker" />
        <file src="js\plugins\input-mask\*.js" target="content\Scripts\vendor\input-mask" />
        <file src="js\plugins\daterangepicker\*.js" target="content\Scripts\vendor\daterangepicker" />
        <file src="js\plugins\datatables\*.js" target="content\Scripts\vendor\datatables" />
        <file src="js\plugins\colorpicker\*.js" target="content\Scripts\vendor\colorpicker" />
        <file src="js\plugins\misc\**\*.js" exclude="*\modernizr*.js" target="content\Scripts\vendor\misc" />
    </files>
</package>